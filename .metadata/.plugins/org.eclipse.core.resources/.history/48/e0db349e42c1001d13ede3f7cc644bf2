
public class J1_Loop {
	public static void main(String[] args) {
		// 반복문 for
		// for(초기값 ; 종료조건 ; 증가분)
		for (double i = 0; i > -5; i -= 1) { // duplicated
			System.out.println(i);
		}
		for (int l = 2; l < 10; l++) {
			for (int k = 1; k < 10; k++)
				System.out.println(l + "*" + k + "=" + (l * k));
		}
	}
}