import java.util.Scanner;

public class fdasf1 {
	public static void main(String[] args) {
//스캐너 선언
		Scanner A = new Scanner(System.in);
//입력 메시지의 출력 선언 및 입력되는 수의 변수를 선언
		System.out.println("첫번째 숫자를 입력해주세요.");
		double Number1 = A.nextDouble();
		System.out.println("두번째 숫자를 입력해주세요.");
		double Number2 = A.nextDouble();
		System.out.println("세번째 숫자를 입력해주세요.");
		double Number3 = A.nextDouble();
		System.out.println("네번째 숫자를 입력해주세요.");
		double Number4 = A.nextDouble();
		System.out.println("다섯번째 숫자를 입력해주세요.");
		double Number5 = A.nextDouble();
//식을 선언
		double MulNumber = Number1 * Number2 * Number3 * Number4 * Number5;
//곱한값의 출력을 선언
		System.out.println("곱한 값은:" + MulNumber + "입니다.");
	}
}