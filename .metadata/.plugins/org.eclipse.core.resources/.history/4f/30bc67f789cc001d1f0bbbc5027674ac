
public class recursiveFunc2 {
	public static void main(String[] args) {
		int input =5;
		
		System.out.println(factorial(input));
	}
	public static int factorial(int i) {
		if (i == 1) {
			return i;
		}else {
			return factorial(i-1)*i;
		}
	}
//	public static int sumSum(int j) {
