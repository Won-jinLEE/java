import java.util.Scanner;

public class pp2 {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.println("문자열을 입력하세요");
		String asd = s.nextLine();
		
		System.out.println("문자열을 입력하세요");
		String dsa = s.nextLine();
		
		String kkk=asd+dsa;
		System.out.println("합친말은 " +kkk+ "이다");
	}
}
