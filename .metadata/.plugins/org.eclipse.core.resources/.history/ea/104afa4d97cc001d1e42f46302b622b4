
public class RecursiveFucHW1 {
	public static void main(String[] args) {
		int input = 200;

		System.out.println(sumSum(input, 0));
	}

//	public static int factorial(int i) {
//		if (i == 1) {
//			return i;
//		} else {
//			return factorial(i - 1) * i;
//		}
//	}
	public static int sumSum(int j) {
		if (j <100) {
			return j;
		} else {
			return sumSum(--j);
		}
	}
}
