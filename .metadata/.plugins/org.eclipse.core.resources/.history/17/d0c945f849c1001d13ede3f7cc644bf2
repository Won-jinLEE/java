
public class J3_Loop_IF {

	public static void main(String[] args) {
		int a = 10;
		if (a == 10) {
			System.out.println("a==10");
		}
//if에 부합한지 검사하고 아닐시 else의 조건으로 출력됨
		if (a > 10) {
			System.out.println("a>10");
		} else {
			System.out.println("a<=10");
		}
//if에 부합한지 검사하고 아닐시 else if에 부합한지 검사하고 아니면 else으로 출력
		if(a<5) {
			System.out.println("a<5");
		}	else if(a>=11) {
			System.out.println("a>=11");
		}	else {
				System.out.println("else");
			}
		}
	}
