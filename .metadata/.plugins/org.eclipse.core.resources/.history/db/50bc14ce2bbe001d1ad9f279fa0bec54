public class lunch {
	public static void main(String[] args) {
		double First = 10;
		double Second = 2;
		double Third = 20;
		double Fourth = 2147483648;
		boolean TF = 'False';
		char SS = 'a';
		
		System.out.println(First + Second);
		System.out.println(Third);
		System.out.println(Fourth);
		System.out.println(TF);
		System.out.println(SS);
	}
}