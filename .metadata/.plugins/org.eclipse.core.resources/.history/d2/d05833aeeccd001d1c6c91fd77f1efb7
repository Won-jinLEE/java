
public class RecursiveFucHW3 {
	public static void main(String[] args) {
		int input = 10;
		System.out.println(fibonazzi(input, 0));
	}

	public static int fibonazzi(int i, int j) {
		if (i ==1) {
			return 1;
		} else if (i == 2) {
			return 1;
		} else {
			return fibonazzi(i-2, j)+fibonazzi(i-1, j);
		}
	}
}
