import java.util.Arrays;

public class J4_Loop {
	public static void main(String[] args) {
		int a = 3;
		int b = 4;
		int temp = 0;
		
		temp = a;
		a = b;
		b = temp;
		
		int c[] = { 4, 5, 6, 1, 2 };
		Arrays.sort(c);
		System.out.println(Arrays.toString(c));
		System.out.println(multiply(a, b));
	}
	public static int multiply(int a, int b) {
		return a * b;
	}
}
